{
  "name": "YLLibrary",
  "version": "0.0.7",
  "summary": "yile library",
  "description": "                       Yile library\n",
  "homepage": "http://git.easeus.com/iosframework/ylkit",
  "license": "MIT",
  "authors": {
    "何勇": "40924497@qq.com"
